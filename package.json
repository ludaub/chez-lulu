{
  "name": "chez-lulu",
  "description": "A personal and interactive cocktail menu",
  "version": "0.0.1",
  "author": "ludaub",
  "homepage": "https://chez-lulu-bar.web.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ludaub/chez-lulu.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "npm run test -- --browsers=ChromeHeadless",
    "test:ci": "npm run test:headless -- --code-coverage --watch=false",
    "lint": "run-s -c lint:*",
    "lint:ng": "ng lint",
    "lint:prettier": "npm run prettier -- --check",
    "lint:style": "stylelint \"**/*.{css,less,sass,scss}\"",
    "format": "run-s -c format:*",
    "format:ng": "ng lint --fix",
    "format:prettier": "npm run prettier -- --write",
    "format:style": "npm run lint:style -- --fix",
    "prettier": "prettier \"**/*.{js,ts,html,css,less,sass,scss,json,yml,md}\"",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.1",
    "@angular/cdk": "^13.0.1",
    "@angular/common": "~13.0.1",
    "@angular/compiler": "~13.0.1",
    "@angular/core": "~13.0.1",
    "@angular/forms": "~13.0.1",
    "@angular/material": "^13.0.1",
    "@angular/platform-browser": "~13.0.1",
    "@angular/platform-browser-dynamic": "~13.0.1",
    "@angular/router": "~13.0.1",
    "@angular/service-worker": "~13.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular-eslint/builder": "^13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin": "^13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin-template": "^13.0.0-alpha.0",
    "@angular-eslint/schematics": "^13.0.0-alpha.0",
    "@angular-eslint/template-parser": "^13.0.0-alpha.0",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "~13.0.1",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^4.0.2",
    "eslint-plugin-rxjs-angular": "^2.0.0",
    "husky": "^7.0.2",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "puppeteer": "^11.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "~4.4.4"
  },
  "lint-staged": {
    "*.{js,ts,html}": "eslint --cache --fix",
    "*.{css,less,sass,scss}": "stylelint --fix",
    "*.{js,ts,html,css,less,sass,scss,json,yml,md}": "prettier --write"
  }
}
