{
  "name": "lulus-bar",
  "description": "A personal and interactive cocktail menu",
  "version": "0.0.1",
  "author": "ludaub",
  "homepage": "https://github.com/ludaub/lulus-bar",
  "repository": {
    "type": "git",
    "url": "https://github.com/ludaub/lulus-bar.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "run-s lint:*",
    "lint:ng": "ng lint",
    "lint:prettier": "npm run prettier -- --check",
    "lint:style": "stylelint \"**/*.{css,less,sass,scss}\"",
    "format": "run-s format:*",
    "format:ng": "ng lint --cache --fix",
    "format:prettier": "npm run prettier -- --write",
    "format:style": "npm run lint:style -- --fix",
    "prettier": "prettier \"**/*.{html,css,less,sass,scss,js,ts,json,yml,md}\"",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@angular/service-worker": "~12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^3.3.7",
    "eslint-plugin-rxjs-angular": "^1.0.6",
    "husky": "^7.0.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,html}": "eslint --cache --fix",
    "*.{css,less,sass,scss}": "stylelint --fix"
  }
}
